# === REQUIRED ===
DATABASE_URL=postgresql://user:pass@host:5432/db

# Auth
JWT_SECRET=CHANGE_ME_SUPER_RANDOM

# OxaPay
OXAPAY_MERCHANT_KEY=YOUR_OXAPAY_MERCHANT_KEY
OXAPAY_SELLER_KEY=YOUR_OXAPAY_SELLER_KEY
OXAPAY_WEBHOOK_SECRET=CHANGE_ME_RANDOM

# URL
# Optional on Vercel (auto-detected from request headers). Keep it set if you use a custom domain.
NEXT_PUBLIC_BASE_URL=https://your-project.vercel.app

# OpenClaw PRO subscription (UI paywall; access to Gateway still by user's OpenClaw token)
PRO_PRICE_USDT=10

# === FILE STORAGE ===
# Choose one:
# 1) Supabase Storage (recommended)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY
SUPABASE_STORAGE_BUCKET=agents

# 2) Cloudflare R2 (S3 compatible)
R2_ENDPOINT=https://your-account.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your-key-id
R2_SECRET_ACCESS_KEY=your-secret-key
R2_BUCKET=agent-marketplace
R2_PUBLIC_BASE_URL=https://pub-your-account.r2.dev

# Optional override: supabase|r2 (auto-detected if SUPABASE_* present)
STORAGE_BACKEND=supabase
